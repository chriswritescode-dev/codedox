# Frontend Environment Variables
# Copy this file to .env and update the values

# API Configuration
# For development with proxy: use relative path /api
# For production: use full URL like https://api.example.com
VITE_API_URL=/api

# WebSocket Configuration
# For development with proxy: use relative path /ws
# For production: use full URL like wss://api.example.com/ws
VITE_WS_URL=/ws

# Development Proxy Target (only used in development)
# This is where your backend API is running
VITE_API_PROXY_TARGET=http://localhost:8000